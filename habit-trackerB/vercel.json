{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "VERCEL": "1",
      "NODE_ENV": "production",
      "LOG_RESPONSE_BODY": "false"
    }
  }
